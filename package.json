{
	"name": "directus-extension-interface-json-form",
	"description": "An interface extension for editing nested JSON data",
	"icon": "account_tree",
	"version": "1.0.8",
	"license": "GPL-3.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-interface",
		"json",
		"form"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "interface",
		"path": "dist/index.js",
		"source": "src/index.js",
		"host": ">=10.10.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Xchos/directus-extension-interface-json-form"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "12.1.3",
		"vue": "^3.5.13"
	}
}
